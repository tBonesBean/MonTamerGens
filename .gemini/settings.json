{
	"gemini": {
		"version": "3.1.0",
		"settings": {
			"theme": "dark",
			"fontSize": 14,
			"showLineNumbers": true,
			"autoSaveInterval": 5,
			"tabSize": 4,
			"enableLinting": true,
			"defaultLanguage": "en-US",
			"backupEnabled": true,
			"mcpsync": {
				"enabled": true,
				"interval": 10,
				"lastSync": "2024-06-15T12:00:00Z",
				"conflictResolution": "prompt",
				"servers": [
					{
						"name": "IntelliHub",
						"remotehost": "vision:SecureIntelliHubServer"
					}
				]
			}
		},
		"plugins": [
			"spellChecker",
			"autoComplete",
			"codeFormatter",
			"intelligentSuggestions",
			"serverSync",
			"versionControl",
			"themeManager",
			"IntelliHubIntegration",
			"markdownPreview",
			"liveCollaboration",
			"MCPSyncPlugin"
		]
	},
	"MCPSyncPlugin": {
		"version": "1.0.0",
		"settings": {
			"syncInterval": 10,
			"lastSync": "2024-06-15T12:00:00Z",
			"conflictResolution": "prompt",
			"servers": {
				"IntelliHub": {
					"command": "C:\\Services\\IntelliHubMCP\\intellihub_tool\\.venv\\Scripts\\python.exe",
					"args": ["C:\\Services\\IntelliHubMCP\\intellihub_tool\\stdio_server.py", "--mcp"]
				}
			}
		}
	},
	"IntelliHubIntegration": {
		"version": "1.0.0",
		"settings": {
			"enabled": true,
			"server": "vision:SecureIntelliHubServer"
		}
	}
}